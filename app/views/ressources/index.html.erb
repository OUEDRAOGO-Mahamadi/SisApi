<p id="notice"><%= notice %></p>

<h1>Ressources</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Date creation</th>
      <th>Derniere date maj</th>
      <th>Date debut publication</th>
      <th>Date fin publication</th>
      <th>Statut</th>
      <th>Video</th>
      <th>Image</th>
      <th>Description</th>
      <th>Url</th>
      <th>Format</th>
      <th>Langue</th>
      <th>Mot cle</th>
      <th>Utilisateur</th>
      <th>Unite administrative</th>
      <th>Thematique</th>
      <th>Type systeme irrigue</th>
      <th>Type ressource</th>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ressources.each do |ressource| %>
      <tr>
        <td><%= ressource.nom %></td>
        <td><%= ressource.date_creation %></td>
        <td><%= ressource.derniere_date_maj %></td>
        <td><%= ressource.date_debut_publication %></td>
        <td><%= ressource.date_fin_publication %></td>
        <td><%= ressource.statut %></td>
        <td><%= ressource.video %></td>
        <td><%= ressource.image %></td>
        <td><%= ressource.description %></td>
        <td><%= ressource.url %></td>
        <td><%= ressource.format_id %></td>
        <td><%= ressource.langue_id %></td>
        <td><%= ressource.mot_cle_id %></td>
        <td><%= ressource.utilisateur_id %></td>
        <td><%= ressource.unite_administrative_id %></td>
        <td><%= ressource.thematique_id %></td>
        <td><%= ressource.type_systeme_irrigue_id %></td>
        <td><%= ressource.type_ressource_id %></td>
        <td><%= ressource.profile_id %></td>
        <td><%= link_to 'Show', ressource %></td>
        <td><%= link_to 'Edit', edit_ressource_path(ressource) %></td>
        <td><%= link_to 'Destroy', ressource, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ressource', new_ressource_path %>
