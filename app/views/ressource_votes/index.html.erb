<p id="notice"><%= notice %></p>

<h1>Ressource Votes</h1>

<table>
  <thead>
    <tr>
      <th>Ressource</th>
      <th>Vote</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ressource_votes.each do |ressource_vote| %>
      <tr>
        <td><%= ressource_vote.ressource_id %></td>
        <td><%= ressource_vote.vote_id %></td>
        <td><%= link_to 'Show', ressource_vote %></td>
        <td><%= link_to 'Edit', edit_ressource_vote_path(ressource_vote) %></td>
        <td><%= link_to 'Destroy', ressource_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ressource Vote', new_ressource_vote_path %>
