<p id="notice"><%= notice %></p>

<h1>Expert Sous Types</h1>

<table>
  <thead>
    <tr>
      <th>Sous type expert</th>
      <th>Utilisateur</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expert_sous_types.each do |expert_sous_type| %>
      <tr>
        <td><%= expert_sous_type.sous_type_expert_id %></td>
        <td><%= expert_sous_type.utilisateur_id %></td>
        <td><%= link_to 'Show', expert_sous_type %></td>
        <td><%= link_to 'Edit', edit_expert_sous_type_path(expert_sous_type) %></td>
        <td><%= link_to 'Destroy', expert_sous_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

