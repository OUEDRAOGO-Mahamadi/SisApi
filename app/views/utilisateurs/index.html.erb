<p id="notice"><%= notice %></p>

<h1>Utilisateurs</h1>

<table>
  <thead>
    <tr>
      <th>Telephone</th>
      <th>Email</th>
      <th>Nom</th>
      <th>Prenom</th>
      <th>Fonction</th>
      <th>Password</th>
      <th>Status</th>
      <th>Structure</th>
      <th>Specialite</th>
      <th>Categorie</th>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @utilisateurs.each do |utilisateur| %>
      <tr>
        <td><%= utilisateur.telephone %></td>
        <td><%= utilisateur.email %></td>
        <td><%= utilisateur.nom %></td>
        <td><%= utilisateur.prenom %></td>
        <td><%= utilisateur.fonction %></td>
        <td><%= utilisateur.password %></td>
        <td><%= utilisateur.status %></td>
        <td><%= utilisateur.structure_id %></td>
        <td><%= utilisateur.specialite_id %></td>
        <td><%= utilisateur.categorie_id %></td>
        <td><%= utilisateur.profile_id %></td>
        <td><%= link_to 'Show', utilisateur %></td>
        <td><%= link_to 'Edit', edit_utilisateur_path(utilisateur) %></td>
        <td><%= link_to 'Destroy', utilisateur, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Utilisateur', new_utilisateur_path %>
