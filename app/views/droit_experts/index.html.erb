<p id="notice"><%= notice %></p>

<h1>Droit Experts</h1>

<table>
  <thead>
    <tr>
      <th>Libelle</th>
      <th>Creer</th>
      <th>Lire</th>
      <th>Modifier</th>
      <th>Supprimer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @droit_experts.each do |droit_expert| %>
      <tr>
        <td><%= droit_expert.libelle %></td>
        <td><%= droit_expert.creer %></td>
        <td><%= droit_expert.lire %></td>
        <td><%= droit_expert.modifier %></td>
        <td><%= droit_expert.supprimer %></td>
        <td><%= link_to 'Show', droit_expert %></td>
        <td><%= link_to 'Edit', edit_droit_expert_path(droit_expert) %></td>
        <td><%= link_to 'Destroy', droit_expert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

