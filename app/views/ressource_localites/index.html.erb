<p id="notice"><%= notice %></p>

<h1>Ressource Localites</h1>

<table>
  <thead>
    <tr>
      <th>Ressource</th>
      <th>Sous localite</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ressource_localites.each do |ressource_localite| %>
      <tr>
        <td><%= ressource_localite.ressource_id %></td>
        <td><%= ressource_localite.sous_localite_id %></td>
        <td><%= link_to 'Show', ressource_localite %></td>
        <td><%= link_to 'Edit', edit_ressource_localite_path(ressource_localite) %></td>
        <td><%= link_to 'Destroy', ressource_localite, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

